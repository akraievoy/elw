#* @vtlvariable name="courses" type="elw.vo.Course[]" *#
#parse("t/elw.macro.vm")
<html>
<head>
	<meta content="text/html; charset=UTF-8">
	<title>Course List : ELW Admin</title>
	<link href="../css/elw.css" type="text/css" rel="stylesheet"/>
	<script type="text/javascript" src="../js/jquery.js"></script>
	<script type="text/javascript" src="../js/jquery.sha1.js"></script>
</head>
<body style="text-align: left;">
<div id="header">
	<div id="nav"><a href="index">Index</a> &gt; <span style="font-weight: bold">Courses</span></div>
	#elw_cred_adm($auth)
	<div id="header_hr"> </div>
</div>
#elw_messages($elw_messages)
<table><tbody>
#foreach($course in $courses)
<tr>
	<td style="padding-left: 2em;">${course.name}</td>
	<td style="padding-left: 1em; padding-right: 1em;">[&nbsp;<a href="course?elw_ctx=c--${course.id}">Structure</a>&nbsp;]</td>
	<td style="padding-left: 1em; padding-right: 1em;">[&nbsp;<a href="course?elw_ctx=c--${course.id}&test=true">Structure + Test</a>&nbsp;]</td>
</tr>
#end
</tbody></table>
</body>
</html>
